<div class="auth-layout">
  <div class="auth-wrapper-layout">
    <h2 class="auth-title">ログイン</h2>
    <h3 class="normal-auth">通常のログイン</h3>
    <%= form_for(:session, url:"sign_in", class: "auth-form") do |f| %>
      <%= f.email_field :email, placeholder: "email" %></br>
      <%= f.password_field :password, placeholder: "password" %></br>
      <%= f.submit "ログイン", class: "form-submit" %>
    <% end %>
  </div>
</div>
